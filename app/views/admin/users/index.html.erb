<h1>Current List of <%=  owned_restaurant.name %> Users</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Nickname</th>
      <th>Email</th>
      <th>Current Role (click to edit)</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @users.each do |user| %>
        <td><%= user.first_name %>&nbsp<%= user.last_name %></td>
        <td><%= user.nickname %></td>
        <td><%= user.email %></td>
        <td><%= link_to user.role, edit_restaurant_admin_user_path(owned_restaurant, user) %></td>
        <td>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
